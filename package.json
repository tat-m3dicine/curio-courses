{
  "name": "courses",
  "version": "2.0.0",
  "scripts": {
    "prodstart": "export NODE_ENV=production && npm run start",
    "stagingstart": "export NODE_ENV=staging && npm run start",
    "teststart": "export NODE_ENV=test && npm run start",
    "dev": "./node_modules/.bin/nodemon -L -e .ts -w src --exec \"./node_modules/ts-node/dist/bin.js src\"",
    "build": "./node_modules/.bin/tsc --p tsconfig.json",
    "start": "node dist",
    "prestart": "npm install",
    "pretest": "npm install --registry https://npr.saal.ai",
    "test": "./node_modules/.bin/nyc ./node_modules/.bin/mocha 'test/**/*.spec.ts'",
    "lint": "./node_modules/.bin/tslint src/*/*.ts --format codeframe --project ./tsconfig.json",
    "fix-lint": "./node_modules/.bin/tslint src/*/*.ts --fix",
    "type-check": "./node_modules/.bin/tsc --noEmit"
  },
  "dependencies": {
    "@saal-oryx/distributed-cache": "^1.0.0",
    "@saal-oryx/message-bus": "^0.0.4",
    "@saal-oryx/unit-of-work": "^0.5.2",
    "bson": "^4.0.1",
    "continuation-local-storage": "^3.2.1",
    "fastest-validator": "^0.6.17",
    "jsonwebtoken": "^8.4.0",
    "kafka-streams": "^4.12.0",
    "kafkajs": "^1.10.0",
    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-tree-router": "^0.5.0",
    "log4js": "^5.1.0",
    "mongodb": "^3.2.3",
    "nanoid": "^2.0.3",
    "node-gyp": "^6.0.0",
    "node-rdkafka": "^2.7.1-2",
    "request": "^2.88.0",
    "rebuild": "^0.1.2",
    "semver": "^6.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/continuation-local-storage": "^3.2.1",
    "@types/koa": "^2.0.47",
    "@types/mocha": "^5.2.5",
    "@types/mongodb": "^3.1.17",
    "@types/nanoid": "^2.0.0",
    "@types/node": "^12.0.10",
    "@types/request": "^2.48.1",
    "@types/semver": "^6.0.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "husky": "^2.4.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "most": "^1.7.3",
    "nock": "^10.0.4",
    "nodemon": "^1.18.9",
    "nyc": "^14.1.1",
    "sinon": "^7.2.2",
    "sinon-chai": "^3.3.0",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "tslint": "^5.18.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.2.2"
  }
}
